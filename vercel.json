{
  "rewrites": [
    {
      "source": "/api/webhooks/cal",
      "destination": "/api/webhooks/cal"
    }
  ],
  "headers": [
    {
      "source": "/api/webhooks/cal",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-Cal-Signature-256, cal-signature, Content-Type"
        }
      ]
    }
  ],
  "functions": {
    "src/app/api/webhooks/cal/route": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
} 